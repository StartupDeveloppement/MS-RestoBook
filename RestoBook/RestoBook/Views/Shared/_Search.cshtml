@using (Html.BeginForm("Search", "Home", FormMethod.Get))
{
    <div class="row">
        <div class="col-md-6 col-md-offset-3">
            <div class="input-group">
                @*@Html.TextBox("SearchString", ViewBag.CurrentFilter as string, new { @class = "form-control", placeholder = "Ville, rue, nom du restaurant", required = "required" })*@
                @Html.TextBox("SearchString", Model as string, new { @class = "form-control", placeholder = "Ville, rue, nom du restaurant", required = "required" })
                <span class="input-group-btn">
                    <button type="submit" class="btn btn-default">Recherche</button>
                </span>
            </div>
        </div>
    </div>
}

@Styles.Render("~/Content/Css/jqueryui")
@Scripts.Render("~/bundles/jqueryui")
@Scripts.Render("~/bundles/myautocomplete")

    
